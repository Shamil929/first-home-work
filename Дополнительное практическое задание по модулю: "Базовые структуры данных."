grades = [[5, 3, 3, 5, 4], [2, 2, 2, 3], [4, 5, 5, 2], [4, 4, 3], [5, 5, 5, 4, 5]]
a = {'Джонни', 'Бильбо', 'Стив', 'Хендрик', 'Аарон'}
b=sorted(a)
d=list(b)
c ={d[0] : sum(grades [0]) / len(grades [0]), d[1]: sum(grades [1]) / len(grades [1]),d[2] : sum(grades [2]) / len(grades [2]),d[3] : sum(grades [3]) / len(grades [3]),d[4] : sum(grades [4]) / len(grades [4])}
print(c)
